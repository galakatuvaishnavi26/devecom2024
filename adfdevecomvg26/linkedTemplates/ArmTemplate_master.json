		"ls_source_sql_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_source_sql'"
		},
		"ds_sink_adls_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ds_sink_adls'"
		},
		"ls_source_adls_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_source_adls'"
		},
		"ds_source_blob_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ds_source_blob'"
		},
		"ds_sink_adls_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://adlssidevecom26.dfs.core.windows.net/"
